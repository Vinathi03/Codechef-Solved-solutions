#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	int X;
	while(T--){
	    cin>>X;
	    if((X>=1) && (X<=10)){
	        if((X>=1) && (X<4)){
	            cout<<"MILD"<<endl;
	        }
	        else if((X>=4) && (X<7)){
	            cout<<"MEDIUM"<<endl;
	        }
	        else if((X>=7) && (X<=10)){
	            cout<<"HOT"<<endl;
	        }
	    }
	    else{
	        cout<<"Enter proper value";
	    }
	}
	return 0;
}
