#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t>0){
	    int n,m=0;
	    cin>>n;
	   int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	        if(a[i]>=1000){
	            m++;
	        }
	    }
	    cout<<m<<endl;
	    t--;
	}
	return 0;
}
